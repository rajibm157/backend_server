{
  "name": "api-server",
  "version": "1.0.0",
  "description": "api server with typescript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec npx ts-node src/index.ts",
    "start:dev": "ts-node-dev --respawn ./src/index.ts",
    "prebuild": "rmdir /S /Q dist",
    "build": "npx tsc -p ./"
  },
  "keywords": [
    "node, express, typescript"
  ],
  "author": "Rajib",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.41",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "_moduleAliases": {
    "_root": "./dist",
    "_config": "./dist/config",
    "_helpers": "./dist/helpers",
    "_errors": "./dist/helpers/errors",
    "_types": "./dist/helpers/interfaces",
    "_controllers": "./dist/controllers",
    "_middleware": "./dist/middleware",
    "_models": "./dist/models",
    "_routes": "./dist/routes",
    "_schemas": "./dist/schemas",
    "_services": "./dist/services"
  }
}
